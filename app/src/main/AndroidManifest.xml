<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.noxgroup.app.learnopengl">

    <application
        android:name=".MyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.LearnOpengl">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".glsurfaceview.GLSurfaceViewActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".image.ImageActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".mirror.MirrorActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".interpolator.InterpolatorActivity"
            android:screenOrientation="portrait" />
    </application>

</manifest>